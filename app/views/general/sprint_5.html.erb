<html>

<head>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <!-- Latest compiled and minified CSS -->
  <link rel="stylesheet" type="text/css" href="/assets/stylesheets/bootstrap.css">
</head>

<%= render "layouts/header" %>


<body>
    <% if current_user %>
      <%= render "layouts/navbar",  title: '',  login: false, logout: true, home: true %>
    <% end %>


  <div class="container">
    <div class="row">
      <div class="page-header">
        <h1>Revision sprint 5</h1>
      </div>
    </div>
  </div>

    <div class="container">

      <!-- Marketing Icons Section -->
      <!-- <div class="row"> -->










        <div class="panel panel-primary">
          <!-- Default panel contents -->
          <div class="panel-heading">Ordenes de compra (solo aceptadas)</div>

          <!-- Table -->
          <table  class="table table-bordered" cellspacing="0" width="100%">
              <thead>
                  <tr>
                    <th>OC emitidas (solo aceptadas)</th>
                    <th>Producto</th>
                    <th>Precio</th>
                    <th>Cant</th>
                    <th>Cant done</th>
                    <th>Factura asociada</th>
                    <th>Pago asociada</th>
                  </tr>
              </thead>

              <tbody id="news-table">
                  <% @flujo_compra.each do |flow| %>
                    <tr>
                      <td><%= flow[:po]["id_cloud"] %></td>
                      <td><%= flow[:po]["product_sku"] %></td>
                      <td><%= flow[:po]["unit_price"] %></td>
                      <td><%= flow[:po]["quantity"] %></td>
                      <td><%= flow[:po]["quantity_done"] %></td>
                      <td><%= flow[:invoice]["id_cloud"] if flow[:invoice]  %></td>
                      <td><%= flow[:trx]["id_cloud"] if flow[:trx] %></td>


                    </tr>
                  <% end %>
              </tbody>
          </table>
        </div>




        <div class="panel panel-primary">
          <!-- Default panel contents -->
          <div class="panel-heading">Ordenes de compra recibidas (solo aceptadas)</div>

          <!-- Table -->
          <table  class="table table-bordered" cellspacing="0" width="100%">
              <thead>
                  <tr>
                    <th>OC recibidas (solo aceptadas)</th>
                    <th>Producto</th>
                    <th>Precio</th>
                    <th>Cant</th>
                    <th>Cant done</th>
                    <th>Factura asociada</th>
                    <th>Pago asociada</th>
                  </tr>
              </thead>

              <tbody id="news-table">
                  <% @flujo_venta.each do |flow| %>
                    <tr>
                      <td><%= flow[:po]["id_cloud"] %></td>
                      <td><%= flow[:po]["product_sku"] %></td>
                      <td><%= flow[:po]["unit_price"] %></td>
                      <td><%= flow[:po]["quantity"] %></td>
                      <td><%= flow[:po]["quantity_done"] %></td>
                      <td><%= flow[:invoice]["id_cloud"] if flow[:invoice]  %></td>
                      <td><%= flow[:trx]["id_cloud"] if flow[:trx] %></td>


                    </tr>
                  <% end %>
              </tbody>
          </table>
        </div>






  <script type="text/javascript" src="/assets/js/jquery.js"></script>
  <script type="text/javascript" src="/assets/js/bootstrap.js"></script>


</body>
</html>
